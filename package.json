{
    "name": "blogg-app",
    "version": "1.0.0",
    "scripts": {
        "start": "node backend/server.js",
        "start:admin": "cd frontend/admin && npm start",
        "start:client": "cd frontend/client && npm start",
        "install": "cd frontend/admin && npm install && cd ../client && npm install",
        "dev": "concurrently \"npm run start:admin\" \"npm run start:client\""
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "concurrently": "^7.6.0",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "mongodb": "^6.3.0",
        "mongoose": "^8.0.4"
    }
}
